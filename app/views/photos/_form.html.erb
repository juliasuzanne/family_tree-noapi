<%= form_with model: @photo do |form| %>
  <div>
    <%= form.label :name%>
    <%= form.text_field :name %>
  </div>
  <div>
    <%= form.label :born %>
    <%= form.text_field :born %>
  </div>
  <div>
    <%= form.label :description %>
    <%= form.text_field :description %>
  </div>
  <div>
    <%= form.submit %>
  </div>
<% end %>